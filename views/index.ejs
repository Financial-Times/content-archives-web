<html>

<head>
  <title>Daily exports</title>
  <link rel="stylesheet"
        href="https://www.ft.com/__origami/service/build/v3/bundles/css?components=o-table@^9.2.5,o-fonts@^5.3.3,o-grid@^6.1.5,o-layout@^5.2.3,o-header-services@^5.2.3,o-footer-services@^4.2.3,o-autoinit@^3.1.3&brand=internal&system_code=upp-public-exports"/>
  <link rel="stylesheet" type="text/css" href="/static/main.css"/>
</head>

<body>
<div class="o-layout o-layout--landing" data-o-component="o-layout">
  <div class="o-layout__header">
    <header class="o-header-services" data-o-component="o-header">
      <div class="o-header-services__top o-header-services__container">
        <div class="o-header-services__logo"></div>
        <div class="o-header-services__title">
          <h1 class="o-header-services__product-name"><a href="/">Content &amp; Metadata Platform</a></h1><span
                  class="o-header-subrand__product-tagline ">Daily exports</span>
        </div>
      </div>
    </header>
  </div>

  <div class="o-layout__main o-layout-typography">
    <h4>Downloadable Content and Concept archives</h4>
    <table class="o-table o-table--row-stripes" data-o-component="o-table">
      <thead>
      <th>File name</th>
      <th>Size</th>
      <th>Last modified</th>
      </thead>
      <tbody>
      <% archives.forEach((archive) => { %>
        <tr>
          <td><a href="/download/<%= archive.name %>"><%= archive.name %></a></td>
          <td><%= archive.size %></td>
          <td><%= archive.lastModified %></td>
        </tr>
      <% }); %>
      </tbody>
    </table>
  </div>

  <div class="o-layout__footer">
    <footer class="o-footer-services">
      <div class="o-footer-services__container">
        <div class="o-footer-services__wrapper o-footer-services__wrapper--top">
          <a class="o-footer-services__icon-link o-footer-services__icon-link--github" href="https://github.com/Financial-Times/upp-public-exports">View project on GitHub</a>
        </div>
      </div>
      <div class="o-footer-services__container">
        <div class="o-footer-services__wrapper o-footer-services__wrapper--legal">
          <div class="o-footer-services__links">
            <a href="http://help.ft.com/help/legal-privacy/cookies/">Cookies</a>
            <a href="http://help.ft.com/help/legal-privacy/copyright/copyright-policy/">Copyright</a>
            <a href="http://help.ft.com/help/legal-privacy/privacy/" class="o-footer-services__bulletted-link">Privacy</a>
            <a href="http://help.ft.com/help/legal-privacy/terms-conditions">Terms &amp; Conditions</a>
          </div>
          <p><span>&#xA9; THE FINANCIAL TIMES LTD.</span> FT and &apos;Financial Times&apos; are trademarks of The Financial Times Ltd.</p>
        </div>
      </div></footer>
  </div>
</div>
</body>
</html>
